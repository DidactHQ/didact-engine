{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Didact": "Data Source=localhost;Initial Catalog=Didact;Integrated Security=true"
  },
  "Didact": {
    "MigrateDatabase": true,
    "DatabaseProvider": "SqlServer",
    "StandardQueues": [
      {
        "Name": "Default",
        "WorkerPartitionPercentage": 50
      },
      {
        "Name": "StandardQueue1",
        "WorkerPartitionPercentage": 25
      }
    ],
    "Directives": [
      "Worker",
      "Scheduler",
      "Storage Poller",
      "Enqueuer"
    ]
  }
}